{
  "name": "@nota-filler/root",
  "private": true,
  "scripts": {
    "format": "prettier . --ignore-unknown --write",
    "dev": "pnpm run --filter @nota-filler/app dev",
    "build": "pnpm run --filter @nota-filler/app build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1"
  }
}
